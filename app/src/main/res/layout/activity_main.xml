<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tvBalance"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="325dp"
        android:layout_marginBottom="637dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/income" />

    <TextView
        android:id="@+id/tvMoneyIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="260dp"
        android:layout_marginBottom="24dp"
        android:text="MONEY IN"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.69"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5" />

    <ImageView
        android:id="@+id/tvTrans"
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:layout_marginStart="230dp"
        android:layout_marginTop="170dp"
        android:layout_marginEnd="70dp"
        android:layout_marginBottom="497dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/transactions" />

    <TextView
        android:id="@+id/tvTransacations"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="24dp"
        android:text="TRANSACTIONS"
        app:layout_constraintBottom_toBottomOf="@+id/tvTrans"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvTrans"
        app:layout_constraintTop_toTopOf="@+id/tvTrans" />

    <TextView
        android:id="@+id/tvTransNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8"
        app:layout_constraintBottom_toBottomOf="@+id/tvTrans"
        app:layout_constraintTop_toBottomOf="@+id/tvTransacations"
        tools:layout_editor_absoluteX="320dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kes 42400"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/tvMoneyIn"
        tools:layout_editor_absoluteX="96dp" />

    <ImageView
        android:id="@+id/vSalary"
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:layout_marginStart="6dp"
        android:layout_marginEnd="355dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/income_trx"
        tools:layout_editor_absoluteY="234dp" />

    <ImageView
        android:id="@+id/tvMoneyOutImg"
        android:layout_width="50dp"
        android:layout_height="100dp"

        android:layout_marginStart="85dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="113dp"
        android:layout_marginBottom="70dp"
        app:layout_constraintBottom_toTopOf="@+id/tvTrans"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/expense" />

    <TextView
        android:id="@+id/rvMoneyIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="33dp"
        android:text="MONEYOUT"
        app:layout_constraintBottom_toBottomOf="@+id/tvMoneyOutImg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvMoneyOutImg"
        app:layout_constraintTop_toTopOf="@+id/tvMoneyOutImg" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="33dp"
        android:text="KES 23700"
        app:layout_constraintBottom_toBottomOf="@+id/tvMoneyOutImg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvMoneyOutImg"
        app:layout_constraintTop_toTopOf="@+id/tvMoneyOutImg" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="148dp"
        android:layout_marginBottom="26dp"
        app:layout_constraintBottom_toTopOf="@+id/vSalary"
        app:layout_constraintEnd_toStartOf="@+id/tvTrans"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:srcCompat="@drawable/balance" />

</androidx.constraintlayout.widget.ConstraintLayout>